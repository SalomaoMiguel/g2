<!DOCTYPE html>
<html lang="pt-br">
<head>
  <meta charset="utf-8">
  <title>G2</title>
  <meta name="SalomaoCruz" content="9rts" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="keywords" content="G2 - Site G2-teste" />
  <meta name="description" content="G2 - Site G2-teste" />
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>
  <%= stylesheet_link_tag 'sort', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'sort', 'data-turbolinks-track': 'reload' %>
</head>
<!-- Body Start -->
<body data-spy="scroll" data-target="#navbar" data-offset="98">
<!-- page loading -->
<div id="loading">
  <div class="load-circle"><span class="one"></span></div>
</div>
<!-- end page loading -->
<!-- Header -->
<header class="header-nav header-white">
  <div class="fixed-header-bar">
    <div class="container container-large">
      <div class="navbar navbar-default navbar-expand-lg main-navbar">
        <div class="navbar-brand">
          <a href="#" title="Mombo" class="logo">
            <%= image_tag("logo/logo_white.png", class:'light-logo', alt:'', title:'', width: 150, height: 50) %>
            <%= image_tag("logo/logo.png", class:'dark-logo', alt:'', title:'', width: 150, height: 50) %>
          </a>
        </div>
        <div class="navbar-collapse justify-content-end collapse" id="navbar-collapse-toggle">
        </div>
        <div class="extra-menu d-flex align-items-center">
          <div class="d-none d-md-block h-btn m-35px-l">
            <a class="m-btn  m-btn-theme m-btn-radius" href="#">ENTRAR</a>
          </div>
          <button type="button" class="navbar-toggler collapsed" data-toggle="collapse" data-target="#navbar-collapse-toggle" aria-expanded="false">
            <span class="icon-bar"></span>
          </button>
        </div>
      </div>
    </div>
  </div>
</header>
<!-- Header End -->
<!-- Main -->
<main>
  <!-- Page Title -->
  <section class="section bg-center bg-cover bg-fixed effect-section" >
    <div class="mask theme-bg opacity-9"></div>
    <div class="container">
      <div class="row justify-content-center p-50px-t">
        <div class="col-lg-8 text-center">
          <h2 class="white-color h1 m-20px-b">G2 - Portal de Notícias</h2>
          <ol class="breadcrumb white justify-content-center">
            <li><a href="index.html">Notícias</a></li>
            <li class="active">Em tempo real</li>
          </ol>
        </div>
      </div>
    </div>
  </section>
  <!-- En Page Title -->

  <!-- Section -->
  <section class="section">
    <div class="container">
      <%= yield %>
    </div>
  </section>
  <!-- End Section -->
</main>
<!-- main end -->
<!-- Footer-->
<footer class="dark-bg footer">
  <div class="footer-bottom footer-border-light">
    <div class="container">
      <div class="row">
        <div class="col-md-6 text-center text-md-right">
          <ul class="nav justify-content-center justify-content-md-start m-5px-tb links-white font-small footer-link-1">
            <li><a>G2 - News</a></li>
            <li><a></a></li>
            <li><a href="https://g1.globo.com/" target="_blank">Portal G1 - Modelo Real</a></li>
          </ul>
        </div>
        <div class="col-md-6 text-center text-md-right">
          <p class="m-0px font-small white-color-light">© 2019 copyright all right reserved</p>
        </div>
      </div>
    </div>
  </div>
</footer>
<!-- footer End -->
</body>
</html>